/* -*- ld-script -*- */

/* Linker script configuration for TARGET=at91sam7s256 */

/* Specify the memory areas for AT91SAM7S256: */
MEMORY 
{
    flash  : ORIGIN = 0x00100000, LENGTH = 256K    /* FLASH EPROM        */    
    ram    : ORIGIN = 0x00200000, LENGTH = 64K     /* static RAM area    */
}

/* Reserve 4k for stacks, the rest is heap.  See also _sbrk() */
_stack_end   = 0x20FFFC;
_stack_begin = 0x20F000;



